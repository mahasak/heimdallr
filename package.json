{
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "dev:migrate": "prisma migrate dev --name migration && prisma migrate deploy",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json pages/api",
    "build:nexus-typegen": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only schema",
    "build2": "npm run build:nexus-typegen && next build"
  },
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@apollo/react-ssr": "^4.0.0",
    "@prisma/client": "^2.25.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "apollo-link-schema": "^1.2.5",
    "apollo-server-micro": "^2.25.2",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "graphql-tag": "^2.12.5",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "latest",
    "nexus": "^1.1.0",
    "nexus-prisma": "^0.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-node": "^10.1.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "prisma": "^2.25.0",
    "typescript": "^4.3.5"
  },
  "license": "MIT"
}
